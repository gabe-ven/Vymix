{
  "expo": {
    "name": "Vymix",
    "slug": "Vymix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "vymix",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
      "backgroundColor": "#120c2c"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
        "bundleIdentifier": "com.gabe.vymix",
      "googleServicesFile": "./GoogleService-Info.plist",
      "usesAppleSignIn": true,
      "appleTeamId": "P438329H38"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#120c2c"
      },
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "package": "com.gabe.Vymix"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app-icon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Vymix to access your photos to set playlist covers.",
          "cameraPermission": "Allow Vymix to use your camera to take a cover photo.",
          "microphonePermission": "Allow Vymix to access the microphone if recording video."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "vymix"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bce9cf03-93ec-4b76-99a4-9daf391931b3"
      },
      "FIREBASE_API_KEY": "AIzaSyCvfNiiiDyMJGIWy1N9UZQwclGTbyzRErM",
      "FIREBASE_AUTH_DOMAIN": "vymix-d72ed.firebaseapp.com",
      "FIREBASE_PROJECT_ID": "vymix-d72ed",
      "FIREBASE_STORAGE_BUCKET": "vymix-d72ed.appspot.com",
      "FIREBASE_MESSAGING_SENDER_ID": "940533360034",
      "FIREBASE_APP_ID": "1:940533360034:web:3bcbc9b9d76defd1a3fefb",
